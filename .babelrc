{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": false,
        "regenerator": true,
        "corejs": false
      }
    ],
    [
      "@babel/plugin-transform-regenerator",
      {
        "async": false
      }
    ]
  ]
}
